{
  "name": "codemg-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "export APP_ENV=dev;npm run nodemon",
    "start": "export APP_ENV=production; node -r dotenv/config -r ts-node/register/transpile-only -r tsconfig-paths/register build/index.js",
    "build": "tsc",
    "migrate": "prisma migrate deploy",
    "nodemon": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' -r dotenv/config -r tsconfig-paths/register --exec 'ts-node --files' src/index.ts",
    "test": "export APP_ENV=test;jest --silent",
    "test:watch": "export APP_ENV=test; jest --watchAll --no-cache",
    "coverage": "export APP_ENV=test; jest --coverage"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "ts-node -r tsconfig-paths/register ./src/prisma/seed-execute.ts"
  },
  "author": "Suraj Wadje",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.188",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-jwt": "^3.0.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "faker": "^5.5.3",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8",
    "uuid": "^9.0.0"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "@sentry/node": "^7.19.0",
    "@sentry/tracing": "^7.19.0",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.0",
    "http-status": "^1.5.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "prisma": "^4.6.1",
    "winston": "^3.8.2",
    "winston-transport-sentry-node": "^2.7.0",
    "xss-clean": "^0.1.1"
  }
